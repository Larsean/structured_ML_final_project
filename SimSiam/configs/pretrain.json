{
    "model": {
        "name": "pretrained",
        "backbone": "resnet50",
        "latent_dim": 2048,
        "proj_hidden_dim": 2048,
        "pred_hidden_dim": 512
    },
    "data": {
        "path": "data/",
        "input_shape": [
            96,
            96
        ],
        "num_classes": 10
    },
    "train": {
        "epochs": 200,
        "batch_size": 256,
        "lr": 0.05,
        "momentum": 0.9,
        "weight_decay": 0.0001,
        "log_interval": 100
    },
    "device": "cuda"
}